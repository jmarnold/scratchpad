<?xml version="1.0"?>
<bindings>
  <element name="Form">
    <start>this.FormFor&lt;@model&gt;()</start>
    <end>this.EndForm()</end>
  </element>
  <element name="Script">this.ScriptFor('@src')</element>
  <element name="Stylesheet">this.CSS('@href')</element>
  <element name="Image">this.CreateImageFor('@src')</element>
  <element name="LabelFor">this.LabelFor(m =&gt; m.@property)</element>
  <element name="InputFor">this.InputFor(m =&gt; m.@property)</element>
  <element name="RowFor">this.RowFor(m =&gt; m.@property)</element>
</bindings>